{
	"overrides": [
		{
			"files": ["*"],
			"options": {
				"tabWidth": 4,
				"useTabs": true,
				"bracketSameLine": true,
				"printWidth": 90,
				"arrowParens": "always",
				"bracketSpacing": true,
				"experimentalTernaries": true
			}
		}
	],
	"plugins": ["prettier-plugin-organize-imports"]
}
